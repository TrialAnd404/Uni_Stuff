DEF ToggleAnimation4 Script {
eventIn SFTime toggle
eventOut SFTime toggleTime
eventOut SFBool active


field SFNode timer USE AnimationTimer4
field SFNode waitForMe USE WaitTimer
field SFBool state FALSE
field SFTime pause 0

url["javascript:
    function toggle(currentTime) {
        if(!waitForMe.isActive){
        if( state ) {
            state = FALSE;
            pause = currentTime - timer.startTime_changed;
        }
        else {
            state = TRUE;
            timer.set_startTime = currentTime - pause;
            pause = 0;
        }

        timer.enabled = state;
        timer.loop = state;
        toggleTime = currentTime;
        active = state;
        }
    }"]
}

DEF DelaySkript Script{
