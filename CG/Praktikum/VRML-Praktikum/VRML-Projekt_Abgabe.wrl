#ANIMATION 1

DEF AnimationTimer1 TimeSensor{
    cycleInterval 40.0
    enabled FALSE
    loop TRUE
}

DEF ToggleAnimation1 Script {
eventIn SFTime toggle
eventOut SFTime toggleTime
eventOut SFBool active


field SFNode timer USE AnimationTimer1
field SFBool state FALSE
field SFTime pause 0

url["javascript:
    function toggle(currentTime) {
        if( state ) {
            state = FALSE;
            pause = currentTime - timer.startTime_changed;
        }
        else {
            state = TRUE;
            timer.set_startTime = currentTime - pause;
            pause = 0;
        }

        timer.enabled = state;
        timer.loop = state;
        toggleTime = currentTime;
        active = state;
    }"]
}

ROUTE Toggle1.touchTime TO ToggleAnimation1.toggle
ROUTE ToggleAnimation1.toggleTime TO Laeufer1.toggleTime
ROUTE ToggleAnimation1.active TO Laeufer1.active

